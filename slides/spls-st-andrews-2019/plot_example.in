set terminal eps
set xtics 1,1,6 font "Times-Roman, 20"
set key font "Times-Roman, 30"
set xrange [-1:7]
set samples 100000
normal(x, mu, sd) = (1/(sd*sqrt(2*pi)))*exp(-(x-mu)**2/(2*sd**2))
U(x) = x < 0? 0 : x <= 1? 6*((x - (1/2))**2) : 0
cbeta(x,p,q)=ibeta(p,q,x)
f(x,a) = a*x

set output "output/plot.eps"
set yrange [0:3]
set ytics 0,1,6 font "Times-Roman, 20"


plot [0:3] 'data/plot.dat' notitle lt 1 lw 4 lc '#008b8b' with line



